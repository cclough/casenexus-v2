#profile_index_panel_user







  #profile_index_info

    #profile_index_info_avatar
      = avatar_for(current_user,size:"medium")

    #profile_index_info_user

      #profile_index_info_username
        = current_user.username

      #profile_index_info_degreelevel
        %i.icon-star
        = current_user.degree_level_in_words

      #profile_index_info_joined
        %i.icon-clock
        = time_ago_in_words(current_user.created_at)
        ago

      / #profile_index_info_cases_rate
      /   Rate: 2 cases per week


  #profile_index_info_cases
    #profile_index_info_cases_givn
      .profile_index_info_cases_counts_container
        = current_user.case_count_recd
        %span.profile_index_info_cases_text
          cases
    #profile_index_info_cases_recd
      .profile_index_info_cases_counts_container
        = current_user.case_count_givn
        %span.profile_index_info_cases_text
          given
    #profile_index_info_cases_external
      .profile_index_info_cases_counts_container
        = current_user.case_count_external
        %span.profile_index_info_cases_text
          external





  #profile_index_info_actions
    / #profile_index_info_actions_settings{"data-original-title"=>"Your settings", rel: "tooltip", "data-container"=>"body", "data-placement"=>"bottom", class: "application_tooltip"}
    /   = image_tag "icons/profile/settings.png"
    / #profile_index_info_actions_visitors{"data-original-title"=>"Visitors", rel: "tooltip", "data-placement"=>"bottom", class: "application_tooltip"}
    /   %i.icon-user
    /     %span.iconbar-unread
    /       = current_user.visits.count

    #profile_index_info_actions_settings.btn.btn-colour.btn-round{"data-original-title"=>"Your settings", rel: "tooltip", "data-container"=>"body", "data-placement"=>"bottom", class: "application_tooltip"}
      %i.icon-settings

    #profile_index_info_actions_invite.btn.btn-colour.btn-round{"data-original-title"=>"Invite friends", rel: "tooltip", "data-container"=>"body", "data-placement"=>"bottom", class: "application_tooltip"}
      %i.icon-users-outline



    = link_to destroy_user_session_path, id: "profile_index_info_actions_signout", "data-container"=>"body", "data-original-title"=>"Sign out", rel: "tooltip", "data-placement"=>"bottom", class: " btn btn-colour btn-round application_tooltip", method: :delete do
      %i.icon-logout










  #profile_index_friends

    .profile_index_title
      Partners

      - if @friends_requested.count > 0
        %span.application_unread_label_large
          = @friends_requested.count.to_s




      %input#profile_index_friends_action_input{type:"hidden"}


    .spacer20



    #profile_index_friends_friends
      - if @friends_requested.count > 0
        - for friends_requested_item in @friends_requested
          = render partial: "friends_item",locals:{friends_item: friends_requested_item}
      - if @friends.count > 0
        - for friends_item in @friends
          = render partial: "friends_item",locals:{friends_item: friends_item}
      


      - else
        #profile_index_friends_friends_empty
          %div
            No partners yet






#profile_index_main_container 




  #profile_index_panel_calendar


    = render "events/calendar"


      

  #profile_index_panel_feedback

    .profile_index_title
      Feedback

      - if @cases.unread.count > 0
        %span.application_unread_label_large
          = @cases.unread.count.to_s


      #profile_index_feedback_actions


        #profile_index_feedback_actions_analysis.btn.btn-colour.btn-round{"data-container"=>"body", "data-original-title"=>"Itemised results...", rel: "tooltip", "data-placement"=>"bottom", class: "application_tooltip btn btn-colour btn-round"}
          %i.icon-graph-horizontal


        %a#profile_index_feedback_actions_start.btn.btn-colour.btn-round{href:"/console", "data-container"=>"body", "data-original-title"=>"Start new case...", rel: "tooltip", "data-placement"=>"bottom", class: "application_tooltip"}
          %i.icon-arrow-right-1
        


    .spacer20
    
    #profile_index_feedback_cases

      - if @cases.count > 0
        - @cases.each do |cases_item|
          .profile_index_feedback_cases_item{"data-case_id" => cases_item.id, "data-case_date" => cases_item.created_at.strftime("%Y-%m-%d-%H-%I-%S") }
            
            .profile_index_feedback_cases_item_read_highlight{ class: ("unread" unless cases_item.read?) }

            .profile_index_feedback_cases_item_score
              = cases_item.totalscore_percentage.to_f.round(0).to_s + "%"

            .profile_index_feedback_cases_item_user
              .profile_index_feedback_cases_item_username
                = cases_item.interviewer.username
              .profile_index_feedback_cases_item_subject
                = cases_item.subject_function_trunc
            .profile_index_feedback_cases_item_date
              = cases_item.date_fb
            / = link_to_remote cases_path(cases_item), :method => :delete, :confirm => "Are you sure you want to permanently delete this post?", :success => "$(this).up('.profile_index_feedback_cases_item').remove();" do
            /   .close
      - else
        #profile_index_feedback_cases_empty
          %div
            No feedback yet

    #profile_index_feedback_chart
