    .span12
      Last Case Date
    .span3
      = Case.cases_analysis_stats_casedate(current_user, "last")
  .row
    .span12
      Avg Score /40
    .span3
      = Case.cases_analysis_stats_averagescore(current_user)
  .row
    .span12
      Improvement Rate, %
    .span3
      xxx
