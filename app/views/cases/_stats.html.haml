    .span12
      Analytical
    .span3
      = Case.cases_analysis_stats_global("analytical")
  .row
    .span12
      Commercial
    .span3
      = Case.cases_analysis_stats_global("commercial")
  .row
    .span12
      Conclusion
    .span3
      = Case.cases_analysis_stats_global("conclusion")